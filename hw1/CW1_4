import java.util.Scanner;
class Main14 {
    public static void main(String[] args) {
        circle();
    }
    public static void circle() {
        Scanner sc = new Scanner(System.in);
        double a = 10;
        double b = 1;
        String determination;
        double t = new Scanner(System.in).nextDouble();

        if ((t <= a) && (t >= b)) System.out.println("принадлежит");
        else System.out.println("не принадлежит");
        System.out.print("проверить ещё одно число? да - y, нет - n");
        determination = sc.nextLine();
        if (determination.equals("y")) {
            circle();
        }
        else if(determination.equals("n")){return;}
        else System.out.print("введите y/n");
    }
}
