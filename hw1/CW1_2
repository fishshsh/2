import java.util.Scanner;
public class main1 {

    public static void main(String[] args) {
        double a = 2;
        double b = 2;
        double h = 3;
        double v = getVolume(a, b, h);
        System.out.println(v);
    }
    public static double getVolume(double a, double b, double h){
        return a*b*h;
    }


    /*public static void main(String[] args) {
